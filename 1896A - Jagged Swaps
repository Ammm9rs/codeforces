#include <iostream>
#include <bits/stdc++.h>
#include <string>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> lst(n);
        for(int i=0;i<n;i++){
            cin>>lst[i];
        }
        bool changed=true;
        while(changed){
            changed=false;

            int pointer=0;
            while(pointer!=n)
            {
            if(pointer==0){
                pointer+=1;
                continue;
            }
            else{
                if(pointer+1<n && lst[pointer]>lst[pointer-1] && lst[pointer]>lst[pointer+1] ){
                    int temp_a=lst[pointer];
                    lst[pointer]=lst[pointer+1];
                    lst[pointer+1]=temp_a;
                    changed=true;
                }
                pointer+=1;
            }

            }

        }






        
        string s="YES";
        for(int i=0;i<n-1;i++){
            if(lst[i]>lst[i+1]){
                s="NO";
                break;


            }
            
        }
        cout<<s<<'\n';
    }
}
