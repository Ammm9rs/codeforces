#include <iostream>
#include <bits/stdc++.h>
#include <string>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        
        cin>>n;
        int size=n;
        vector<int> lst(size);
        vector<int> answer(size);

        for(int i=0;i<size;i++){
            cin>>lst[i];
        }
        for(int i =0;i<size;i++){
            answer[i]=n+1-(lst[i]);
        }
        for(int i=0;i<size;i++){
            cout<<answer[i]<<" ";
            
        }
        cout<<endl;

    }
    

}
